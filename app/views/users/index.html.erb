<%= render "books/info", user: @current_user, book: @create_book %>

<h2>Users</h2>

<table class = "table">
  <thead>
   <tr>
    <th>画像</th>
    <th>名前</th>
    <th></th>
    <th colspan="3"></th>
   </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= image_tag user.get_profile_image(100,100) %></td>
      <td><%= user.name %></td>
      <td><%= link_to "Show", user_path(user.id) %></td>
    </tr>
    <% end %>
  </tbody>
</table>